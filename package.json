{
    "name": "angular-holiday",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run start:server && npm run start:webapp",
        "start:server": "cd server && brew services run mongodb-community && python3 app.py",
        "stop:server": "cd server && brew services stop mongodb-community",
        "start:webapp": "cd webapp && npm run start"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.6",
        "@auth0/auth0-angular": "^2.2.1",
        "@types/node": "^20.10.4",
        "jwt-decode": "^4.0.0",
        "ngx-simple-modal": "^1.4.17",
        "ngx-toastr": "^18.0.0",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^17.0.0",
        "dotenv-webpack": "^8.0.1"
    }
}
